ansible-playbook -i inventory_file bq_truncate_data.yml

ansible-playbook -i inventory_file gcs_bucket_creation.yml -e "bucket_name=your_bucket_name Project_ID=your_project_id Storage_Class=your-storage_class bucket_location=your_bucket_location"

